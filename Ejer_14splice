function gestionarEmpleados(lista) {
  lista.splice(lista.length, 0, "NuevoEmpleado");
  lista.splice(1, 1);

  lista.splice(2, 1, "EmpleadoReemplazo");

  console.log("Lista de empleados actualizada:");
  console.log(lista);
}

let empleados = ["Jose", "Alan", "Quenet", "Lorena"];
gestionarEmpleados(empleados);
